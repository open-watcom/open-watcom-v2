# DOS
[ DEFAULT type="s" dir="binw" cond="doshost" where="c f77 jc jf77" ]
usr="net.trp"   old="bin" keys="!newdbg"      desc="NETBIOS trap file"
usr="net??.trp"           keys="newdbg"       desc="NETBIOS trap file"
usr="nmp.trp"   old="bin" keys="!newdbg"
usr="nmp??.trp"           keys="newdbg"
usr="nov.trp"   old="bin" keys="!newdbg"
usr="nov??.trp"           keys="newdbg"
usr="par.trp"   old="bin" keys="!newdbg"
usr="par??.trp"           keys="newdbg"
usr="ser.trp"   old="bin" keys="!newdbg"
usr="ser??.trp"           keys="newdbg"
usr="win.trp"   old="bin" keys="!newdbg"
usr="win??.trp"           keys="newdbg"
[ DEFAULT type="s" dir="binw" cond="dostarg" where="c f77 jc jf77" ]
usr="cw.trp"    old="bin" keys="!newdbg"      desc="Debugging trap file for CauseWay"
usr="cw??.trp"            keys="newdbg"       desc="Debugging trap file for CauseWay"
usr="pls.trp"   old="bin" keys="!newdbg"      desc="Debugging trap file for Phar Lap"
usr="pls??.trp"           keys="newdbg"       desc="Debugging trap file for Phar Lap"
usr="rsi.trp"   old="bin" keys="!newdbg"      desc="Debugging trap file for DOS/4GW"
usr="rsi??.trp"           keys="newdbg"       desc="Debugging trap file for DOS/4GW"
usr="hx.trp"              keys="!newdbg"      desc="Debugging trap file for HX"
usr="hx??.trp"            keys="newdbg"       desc="Debugging trap file for HX"
usr="std.trp"   old="bin" keys="!newdbg"      desc="Debugging trap file for 16-bit DOS debugging"
usr="std??.trp"           keys="newdbg"       desc="Debugging trap file for 16-bit DOS debugging"
[ DEFAULT dir="binw" cond="dostarg" where="c f77 jc jf77" ]
type="e" usr="netserv.exe"       old="bin"
type="e" usr="nmpserv.exe"       old="bin"
type="e" usr="novserv.exe"       old="bin"
type="e" usr="parserv.exe"       old="bin"
type="e" usr="serserv.exe"       old="bin"
type="e" usr="tcpserv.exe"                       desc="TCP/IP trap file"
type="e" usr="vdmserv.exe"       old="bin"
type="e" usr="winserv.exe"       old="bin"
type="e" usr="cwhelp.exe"        old="bin"       desc="Debugger help file for CauseWay"
type="s" usr="cwhelp.cfg"        old="bin"
#type="e" usr="plshelp.exp"       old="bin"       desc="Debugger help file for Phar Lap"
#type="e" usr="pedhelp.exp"       old="bin"
type="e" usr="rsihelp.exp"       old="bin"       desc="Debugger help file for DOS/4GW"
[ DEFAULT dir="binw" cond="winhost" where="c jc f77 jf77" ]
type="s" usr="wsrv.pif"

