proj_name = pbide

pbide_autodepends = .AUTODEPEND

!include cproj.mif
!include defrule.mif
!include deftarg.mif

.c: ..

extra_c_flags_win =-zW

dlldbg.exe : dlldbg.obj dlldbg.res
        $(linker) name $^@ $(lflags) file {dlldbg.obj}
        $(rc) -k dlldbg.res $^@

dlldbg.res : ../dlldbg.rc ../debugger.ico
        $(rc) $(rc_flags) $[@ -fo=$@
