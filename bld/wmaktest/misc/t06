all: .symbolic
    rm -f foo
!ifdef __UNIX__
    -rmdir food 2> /dev/null
!else
    -rmdir food 2> NUL
!endif
    echo foo > foo
    rm foo
    echo foo > foo
    chmod -w foo
    -rm foo
    rm -f foo
    rm -f foo
    mkdir food
    -rm -f food
    rm -f -r food
