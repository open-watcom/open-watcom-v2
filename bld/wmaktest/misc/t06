# ----------------------------------------------------
# Test 06: rm tests
# ----------------------------------------------------

!ifdef __UNIX__
nuldev = /dev/null
!else
nuldev = NUL
!endif

all: .symbolic
    rm -f foo
    -rmdir food 2> $(nuldev)
    echo foo > foo
    rm foo
    echo foo > foo
    chmod -w foo
    -rm foo
    rm -f foo
    rm -f foo
    mkdir food
    -rm -f food
    rm -f -r food

