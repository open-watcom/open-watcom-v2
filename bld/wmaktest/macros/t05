# ----------------------------------------------------
# Test 05: Microsoft DeMacro
# ----------------------------------------------------

all: hello.obj

.c.obj:
     @echo $@
     @echo $*
     @echo $**
     @echo $?
     @echo $<
     @echo $(**D)
     @echo $(@B)
     @echo $(@F)
     @echo $(@R)

hello.obj: ../macros/hello.xyz hello.c hello.boo

