proj_name = watcom

watcom_autodepends = .autodepend

!include cproj.mif
!include defrule.mif
!include dllrule.mif
!include deftarg.mif

objs     = clibext.obj
dll_objs = $(dll_subdir)/clibext.obj

.c: ../c

inc_dirs = -I"../h"

!ifndef __WATCOM__
libs = clibext.lib clibexts.lib
!endif

all: $(libs) .symbolic

clibext.lib : $(objs)
    $(bld_lib) $<

clibexts.lib : $(dll_objs)
    $(bld_lib) $<
