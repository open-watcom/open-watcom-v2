proj_name = txt

name = txt

objs = readme.txt freadme.txt areadme.txt

.EXTENSIONS: .txt

all: $(objs) .SYMBOLIC 
    @%null

readme.txt : ../readme.sp
    wsplice -kC $[@ $@

freadme.txt : ../readme.sp
    wsplice -kF77 $[@ $@

areadme.txt : ../areadme.sp
    wsplice $[@ $@

clean: .SYMBOLIC .EXPLICIT
    rm -f *.txt
