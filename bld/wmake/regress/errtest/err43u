# --------------------------------
# ERROR 43: syserr deleting file
# --------------------------------

!ifdef __UNIX__
mkerr=../cmds/mkerr
!else
mkerr=..\cmds\mkerr
!endif

.ERASE

all.43 : first z43.43 last
    @$(mkerr)

first : .symbolic
    @chmod +w all.43
    @%write all.43
    @chmod -w all.43

z43.43 :
    @%null

last : .symbolic
    @%null
