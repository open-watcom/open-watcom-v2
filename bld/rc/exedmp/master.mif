proj_name = exedmp

exedmp_autodepends = .AUTODEPEND

!include cproj.mif
!include defrule.mif
!include deftarg.mif

inc_dirs = -I"../h"

.c: ../c

objs =  main.obj    &
        read.obj    &
        format.obj  &
        param.obj

$(proj_name).exe : $(objs) $(__MAKEFILES__)
!ifndef __WATCOM_TOOLS__
        $(cl) $(clflags) $(objs) $(ldflags)
!else
        $(linker) name $^@ $(lflags) file {$(objs)}
!endif
