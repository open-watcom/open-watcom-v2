proj_name = exedmp

exedmp_autodepends = .AUTODEPEND

!include cproj.mif
!include deftarg.mif
!include defrule.mif

inc_dirs = -I"../h"

.c: ../c

objs =              &
        main.obj    &
        read.obj    &
        format.obj  &
        param.obj

$(proj_name).exe : $(objs)
!ifndef __WATCOM__
        $(cl) $(clflags) $(objs) $(ldflags)
!else
        $(linker) $(lflags) name $^. file { $< }
!endif
