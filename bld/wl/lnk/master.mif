proj_name = wlink_lnk

!ifeq host_subdir binnt64
!ifndef bld64host_nt
skip_build: .symbolic
!endif
!else ifeq host_subdir binl64
!ifndef bld64host_linux
skip_build: .symbolic
!endif
!endif

!ifeq host_subdir binw
wlsystem.lnk : ../specs.sp wlink.lnk
    wsplice -k Pwlsystem $[@ $@
!endif

wlink.lnk : ../wlink.sp ../specs.sp
    wsplice -k Pwlsystem -k $(host_subdir) $< $@

clean : .symbolic
    rm -f wlink.lnk wlsystem.lnk
