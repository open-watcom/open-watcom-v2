type="l"    usr="clibom.lib"    conda="mm"
type="l"    usr="clibs.lib"     conda="ms"  where="c jc"
type="l"    usr="wovl.lib"      conda="mm ml |"
type="s"    usr="cstart_t.obj"  conda="ms"
type="s"    usr="binmode.obj"
type="s"    usr="commode.obj"

type="l"    usr="doslfnc.lib"   conda="mc"  where="c jc"
