type="l"    usr="doslfnom.lib"  conda="mm"
type="l"    usr="doslfns.lib"   conda="ms"  where="c jc"

[ DEFAULT dir="lib286/os2" cond="os2targ tools16" where="c f77 jc jf77" ]
type="l"    usr="dospmc.lib"    conda="mc"  where="c jc"
type="l"    usr="dospmh.lib"    conda="mh"  where="c jc"
type="l"    usr="dospml.lib"    conda="ml"
type="l"    usr="dospmm.lib"    conda="mm"
type="l"    usr="dospms.lib"    conda="ms"  where="c jc"

type="l"    usr="clibc.lib"     conda="mc"  where="c jc"
type="l"    usr="clibh.lib"     conda="mh"  where="c jc"
type="l"    usr="clibl.lib"     conda="ml"
type="l"    usr="clibdll.lib"   conda="ml"
type="l"    usr="clibmtl.lib"   conda="ml"
type="l"    usr="clibm.lib"     conda="mm"
type="l"    usr="clibs.lib"     conda="ms"  where="c jc"
