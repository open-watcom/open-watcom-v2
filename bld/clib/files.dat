###################
# 32-bit Libraries
#
[ DEFAULT dir="lib386/dos" cond="dostarg" where="c f77 jc jf77" ]
type="l"    usr="clib3r.lib"    cond+="3r"
type="l"    usr="clib3s.lib"    cond+="3s"
type="s"    usr="cstrtx3r.obj"  cond+="3r"
type="s"    usr="cstrtx3s.obj"  cond+="3s"
