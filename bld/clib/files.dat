# CLIB piece of install manifest
#
###################
# 16-bit Libraries
#
[ DEFAULT dir="lib286/dos" cond="dostarg tools16" where="c f77 jc jf77" ]
type="l"    usr="clibc.lib"     cond+="mc"  where="c jc"
type="l"    usr="clibh.lib"     cond+="mh"  where="c jc"
