type="l"    usr="clibm.lib"     conda="mm"
type="l"    usr="clibs.lib"     conda="ms"  where="c jc"
type="s"    usr="binmode.obj"
type="s"    usr="commode.obj"

###################
# 32-bit Libraries
#
[ DEFAULT dir="lib386/dos" cond="dostarg" where="c f77 jc jf77" ]
type="l"    usr="clib3r.lib"    conda="3r"
type="l"    usr="clib3s.lib"    conda="3s"
type="s"    usr="cstrtx3r.obj"  conda="3r"
type="s"    usr="cstrtx3s.obj"  conda="3s"
