type="l"    usr="pc98om.lib"    cond+="mm"
type="l"    usr="pc98s.lib"     cond+="ms"  where="c jc"

[ DEFAULT dir="lib286/os2" cond="os2targ tools16" where="c f77 jc jf77" ]
type="l"    usr="dospmc.lib"    cond+="mc"  where="c jc"
