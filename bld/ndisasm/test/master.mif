tree_depth = 4

proj_name = wdistest

host_cpu = $(bld_cpu)
host_os  = $(bld_os)

!include cproj.mif
!include defrule.mif
!include deftarg.mif

!include $(dig_dir)/digcli.mif
!include ../../discli.mif

extra_cpp_flags = $(dig_arch_cpp_flags)

.c: $(dis_srcs);../c

inc_dirs = -I. $(dis_includes)

exetarg_prebuild_objs = $(dis_prereq)
exetarg_objs          = main.obj $(dis_objs)

!include exetarg.mif

distbls.gh : ./disbuild.exe
    @%make echo_execute
    $(noecho)$[@

./disbuild.exe : $(disasm_dir)/core/c/disbuild.c $(disbuild_headers)
    @%make echo_bldccl
    $(bld_ccl) $[@ $(dig_arch_cpp_flags) -I"../h" $(dis_includes) $(bld_cclflags) $(bld_ldflags)
