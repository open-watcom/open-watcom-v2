#-------------------------------------------------------
# run new (native) wgml utility
#-------------------------------------------------------
run_wgml_new : .PROCEDURE .EXPLICIT
!ifeq verbose 0
    @echo wgml $@
!endif
!ifdef __UNIX__
    $(noecho)set GMLINC=$(%OWGMLINC)
    $(noecho)set GMLLIB=$(%OWGMLLIB)
    -wgml $^& "(" file $(wgml_optfile)
!else
    $(noecho)set GMLINC=$(%OWGMLINC:/=\)
    $(noecho)set GMLLIB=$(%OWGMLLIB:/=\)
    -wgml $^& ( file $(wgml_optfile)
!endif

#-------------------------------------------------------
# run Windows help compiler
#-------------------------------------------------------
run_hc_nt : .PROCEDURE .EXPLICIT
!ifeq bld_os linux
    @%create $(wine_hc_batch)
    @%append $(wine_hc_batch) $(hc_nt) -xn $*.hpj
    # this lowercase help file name in wine
    @%append $(wine_hc_batch) ren $@ xxxxxxxx.tmp
    @%append $(wine_hc_batch) ren xxxxxxxx.tmp $@
    wine cmd /c $(wine_hc_batch)
!else ifeq bld_os nt
    $(hc_nt) -xn $*.hpj
    %ren $@ $@    # change file name to lower case
!else ifeq bld_os os2
    $(hc_nt) -xn $*.hpj
    %ren $@ $@    # change file name to lower case
!else
    $(hc_nt) -xn $*.hpj
!endif

