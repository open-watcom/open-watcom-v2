!ifeq wgml_target nt

.EXTENSIONS: .hlp .rtf .gh .gml

.rtf.hlp :
    @%make compile_hpj_nt
    @%make run_hc_nt

{$(root_books)}.gml.rtf &
{$(root_books)}.gml.gh :
    @echo BOOK : $^&
    @%make compile_gml_to_xxx

####################################################
#
# make WIN help files
#
####################################################
!else ifeq wgml_target win

.EXTENSIONS: .hlp .rtf .gh .gml

.rtf.hlp :
    @%make compile_hpj_win
    $(whc) $*.hpj

{$(root_books)}.gml.rtf &
{$(root_books)}.gml.gh :
    @echo BOOK : $^&
    @%make compile_gml_to_xxx

####################################################
#
# make DOS help files
#
####################################################
!else ifeq wgml_target dos

.EXTENSIONS: .ihp .ib .gh .gml

.ib.ihp :
    $(hcdos) -h 25 -w 80 $[@ $@

{$(root_books)}.gml.ib &
{$(root_books)}.gml.gh :
    @echo BOOK : $^&
    @%make compile_gml_to_xxx

####################################################
#
# make OS2 help files
#
####################################################
!else ifeq wgml_target os2

.EXTENSIONS: .hlp .inf .ipf .gh .gml

.ipf.inf :
    $(noecho)set IPFCARTWORK=$($^&_book_incs)
    $(noecho)set WIPFC=$(%OWROOT)/bld/wipfc/helper
    $(ipfc) -q -inf $[@

.ipf.hlp :
    $(noecho)set IPFCARTWORK=$($^&_book_incs)
    $(noecho)set WIPFC=$(%OWROOT)/bld/wipfc/helper
    $(ipfc) -q $[@

{$(root_books)}.gml.ipf &
{$(root_books)}.gml.gh :
    @echo BOOK : $^&
    @%make compile_gml_to_xxx

