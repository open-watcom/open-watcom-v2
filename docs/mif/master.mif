.ps.pdf :
    !$(hc_pdf) -r600 $[@ $@
!  ifdef wikidir
    $(noecho)%copy $^&.pdf $(wikidir)\$^&.pdf
!  endif
! endif

{$(root_books)}.gml.ps :
    @echo BOOK : $^&
    $(noecho)set OWGMLINC=.;$(root_books);$(tmpdir);$(ps_incs);$($^&_book_incs);$(whelp_incs);$(gml_incs)
    $(noecho)set OWGMLLIB=../gml/owhelp
    %make prepare_wgml_opts
!ifdef %OWNEWWGML
    @%make run_wgml_new
!else
    @%make run_wgml
!endif
