prepare_wgml_opts : .PROCEDURE
#--------------------------------------
# common settings for any processing
#--------------------------------------
    @%create $(wgml_optfile)
# add default windowing for NT
#    @%append $(wgml_optfile) ( setsymbol defwinnt 1
# add default windowing for OS/2
